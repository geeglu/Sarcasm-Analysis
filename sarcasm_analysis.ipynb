    "import pandas as pd\n",
    "import numpy as np\n",
    "import json"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "Importing the sarcastic dataset"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "base_path='big_data/'\n",
    "d = json.load(open(base_path+'comments.json'))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "commentsid_labels={}\n",
    "with open(base_path+'train-balanced.csv', encoding=\"utf8\") as f:\n",
    "    i=0\n",
    "    for line in f:\n",
    "        i+=1\n",
    "        values = line.split('|')\n",
    "        com1, com2 = values[1].split()\n",
    "        label1, label2=values[2].split()\n",
    "        commentsid_labels[com1]=int(label1)\n",
    "        commentsid_labels[com2]=int(label2)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "train_comments=[]\n",
    "train_labels=[]\n",
    "for commentid, label in commentsid_labels.items():\n",
    "    train_comments.append( d[commentid]['text'])\n",
    "    train_labels.append(label)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "Finding the max length of the sequences based on the 98% percentile of the length of all sequences"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "ls=[]\n",
    "for c in train_comments:\n",
    "    ls.append(len(c.split()))\n",
    "maxLen=int(np.percentile(ls, 98))"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "Importing the test dataset"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "commentsid_labels={}\n",
    "with open(base_path+'test-balanced.csv', encoding=\"utf8\") as f:\n",
    "    for line in f:\n",
    "        values = line.split('|')\n",
    "        com1, com2 = values[1].split()\n",
    "        label1, label2=values[2].split()\n",
    "        commentsid_labels[com1]=int(label1)\n",
    "        commentsid_labels[com2]=int(label2)\n",
    "test_comments=[]\n",
    "test_labels=[]\n",
    "for commentid, label in commentsid_labels.items():\n",
    "    test_comments.append( d[commentid]['text'])\n",
    "    test_labels.append(label)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "Bulding the embedding matrix based on the 100D version of GloVe"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "embeddings_index={}\n",
    "with open(('glove100.txt'), encoding='utf8') as f:\n",
    "    for line in f:\n",
    "        values = line.split()\n",
    "        word = values[0]\n",
    "        coefs = np.asarray(values[1:], dtype='float32')\n",
    "        embeddings_index[word] = coefs"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [
    {
     "name": "stderr",
     "output_type": "stream",
     "text": [
